{% block javascripts %}
    {% if includeJquery %}
        <script src="{{ asset('bundles/twentystepsdatatables/js/jquery.js') }}"></script>
    {% endif %}
    {% if includeJqueryUi %}
        <script src="{{ asset('bundles/twentystepsdatatables/js/jquery-ui.min.js') }}"></script>
    {% endif %}
    {% if includeJqueryEditable %}
        <script src="{{ asset('bundles/twentystepsdatatables/js/jquery.jeditable.js') }}"></script>
    {% endif %}
    {% if includeJqueryDataTables %}
        <script src="{{ asset('bundles/twentystepsdatatables/js/jquery.dataTables.js') }}"></script>
    {% endif %}
    {% if includeJqueryValidate %}
        <script src="{{ asset('bundles/twentystepsdatatables/js/jquery.validate.js') }}"></script>
    {% endif %}
    <script src="{{ asset('bundles/twentystepsdatatables/js/jquery.dataTables.editable.js') }}"></script>
{% endblock %}